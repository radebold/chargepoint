{
  "common": {
    "name": "chargepoint",
    "version": "0.0.8",
    "title": "ChargePoint",
    "icon": "chargepoint.png",
    "extIcon": "https://raw.githubusercontent.com/radebold/iobroker.chargepoint/main/admin/chargepoint.png",
    "enabled": true,
    "loglevel": "info",
    "mode": "daemon",
