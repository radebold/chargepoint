{
  "common": {
    "name": "chargepoint",
    "version": "0.0.6",
    "title": "ChargePoint",
    "titleLang": {
      "en": "ChargePoint adapter",
      "de": "ChargePoint Adapter"
    },
    "desc": {
      "en": "Monitors ChargePoint charging stations",
      "de": "\u00dcberwacht ChargePoint-Ladestationen"
    },
    "authors": [
      "Thomas"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "main": "main.js",
    "icon": "chargepoint.png",
    "extIcon": "https://raw.githubusercontent.com/radebold/chargepoint/main/admin/chargepoint.png",
    "enabled": true,
    "loglevel": "info",
    "mode": "daemon",
    "type": "energy",
    "connectionType": "cloud",
    "dataSource": "poll",
    "compact": false,
    "adminUI": {
      "config": "json"
    }
  },
  "native": {
    "interval": 10,
    "stationsList": []
  },
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "name": "Connection status",
        "role": "indicator.connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}